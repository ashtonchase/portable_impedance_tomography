	pages = {3282--3285},
	file = {IEEE Xplore Abstract Record:/home/ashton/Zotero/storage/GS3X3H8G/Ferreira et al. - 2011 - AD5933-based electrical bioimpedance spectrometer..html:text/html;IEEE Xplore Full Text PDF:/home/ashton/Zotero/storage/888UJD5Z/Ferreira et al. - 2011 - AD5933-based electrical bioimpedance spectrometer..pdf:application/pdf}

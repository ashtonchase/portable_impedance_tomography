	pages = {1--4},
	file = {IEEE Xplore Abstract Record:/home/ashton/Zotero/storage/5BV2QTNF/6900984.html:text/html;IEEE Xplore Full Text PDF:/home/ashton/Zotero/storage/3XMD95NY/Michalikova and Prauzek - 2014 - A hybrid device for electrical impedance tomograph.pdf:application/pdf}
